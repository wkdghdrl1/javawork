
public class HelloKorea {

	public static void main(String[] args) {

		System.out.println("우리나라만세");	
		System.out.println("우리나라만세");
		System.out.println("korea");
		System.out.println(30+42);
		System.out.println("30+40");
		System.out.println("대한민국만세");
		System.out.println("==================");
		System.out.println("이름 전화번호 주소");
		System.out.println("------------------");
		System.out.println("고길동 121  서울시");
		System.out.println("이몽룡 002  남원시");
		double a = 3e-4;
		System.out.println(a);
	}

}
